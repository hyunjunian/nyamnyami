	function ctl08_FoodQuickSearchControl1_OnClick(el){
		if(el.style.color != '#000000' && el.style.color != 'rgb(0, 0, 0)'){
			el.value='';
			el.style.color='#000000';
		}
		else
		{
			if(navigator.userAgent.indexOf('Chrome') != -1 || navigator.userAgent.indexOf('Apple') != -1)
				el.select();
		}
	}
	function ctl08_FoodQuickSearchControl1_OnFocus(el){
		if(el.style.color == '#000000' || el.style.color == 'rgb(0, 0, 0)'){
			if(!(navigator.userAgent.indexOf('Chrome') != -1 || navigator.userAgent.indexOf('Apple') != -1))
				el.select();
		}
	}
	function ctl08_FoodQuickSearchControl1_OnBlur(el){
		if(el.value==''){
			el.value='검색 입력';
			el.style.color='#BFC3C2';
		}
	}
