	function ctl08_FoodQuickSearchControl1_foodSearch(){
		var exp = document.getElementById('ctl08_FoodQuickSearchControl1_ByFood').value;
		if(exp == '검색 입력') exp = '';
		window.location.href = '/%EC%B9%BC%EB%A1%9C%EB%A6%AC-%EC%98%81%EC%96%91%EC%86%8C/search?q=' + encodeURIComponent(exp);
